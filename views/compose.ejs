<%- include("partials/header") -%>


<h1>Compose</h1>


<form action="/compose" method="POST">


    <div class="form-group">
        <label>Title</label>
        <input type="text" class="form-control" name="postTitle" autocomplete="off">
        <label>Post</label>
        <textarea class="form-control" rows="4" name="postBody" autocomplete="off"></textarea>
    </div>
    <button type="submit" class="btn btn-info ">Submit</button>
</form>








